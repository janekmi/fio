# Example librpma server job
[global]
ioengine=librpma_server
bindname=[IP to listen on]
port=[port to listen on]

# The server side will spawn a single thread, opens and registers the whole
# provided memory. The server will accept a connection, waits for it to end up,
# and close itself.

[server]
filename=[XXX device dax or an existing fsdax file]
